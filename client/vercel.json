{
    "version": 2,
    "name": "your-app-name",
    "public": true,
    "builds": [
        {"src": "package.json", "use": "@vercel/next"}
    ],
    "routes": [
        { "src": "/static/(.*)", "headers": { "cache-control": "s-maxage=63072000,immutable" } },
        { "src": "/favicon.ico", "dest": "/static/favicon.ico" },
        { "src": "/manifest.json", "dest": "/static/manifest.json" }
    ]

}
  